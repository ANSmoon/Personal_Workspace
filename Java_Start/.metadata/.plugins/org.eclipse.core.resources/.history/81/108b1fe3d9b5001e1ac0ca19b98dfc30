package homework;

public class Person {
	String name;
	int age;
	float height;
	String hobby;
	
	
	Person (String name, int age, float height, String hobby){
		this.name = name;
		this.age = age;
		this.height = height;
		this.hobby = hobby;
	}
	
	
}
