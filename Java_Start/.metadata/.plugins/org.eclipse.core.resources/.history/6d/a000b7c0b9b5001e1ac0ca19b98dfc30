package homework;

public class User {
	String id;
	String password;
	String name;
	String email;
	int age;
	
	// 기본 생성자
	public User() {}
	
	// 멤버 변수 초기화를 위해 인자를 받는 생성자
	public User(String id, String password, String name, String email, int age) {
		this = id;
		this = password;
		this = name;
		this = email;
		this = age;
	}
}
