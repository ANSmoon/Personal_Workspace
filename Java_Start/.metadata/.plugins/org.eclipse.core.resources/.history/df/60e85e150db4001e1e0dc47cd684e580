

public class Array05_MinMax {
	public static void main(String[] args) {
		int [] test = {100,20,31,45,50,6,700,89,999,1000};
		
		int min = 10000;
		int max = 0;
		
		for(int i = 0; i < test.length; i++) {
			if(min > test[i])
				min = test[i];
			if(max < test[i])
				max = test[i];
		}
		
		System.out.println(min + " , " + max);
	}
}
