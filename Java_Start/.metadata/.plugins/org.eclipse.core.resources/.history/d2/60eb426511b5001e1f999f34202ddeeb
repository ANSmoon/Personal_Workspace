package homework;

import java.util.Scanner;

public class SpiderWeb {
	public static void main(String[] arge) {
		Scanner sc = new Scanner(System.in);
		
		int size = sc.nextInt();
		
		int[][] arr = new int[size][size];
		
		for(int i = 0; i < size; i++) {
			for(int j = 0; j < size; j++) {
				arr[i][j] = sc.nextInt();
			}
		}
		
		
		
	}
}
