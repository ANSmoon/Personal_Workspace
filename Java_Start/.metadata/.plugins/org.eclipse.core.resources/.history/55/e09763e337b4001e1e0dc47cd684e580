package homework;

import java.util.Arrays;
import java.util.Scanner;

public class SwitchTest {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int n = 0;
		int m = 0;
		int i = 0;
		int j = 0;
		
		n = sc.nextInt();
		m = sc.nextInt();
		
		int[] button = new int[n];
		int[] number = new int[m];
		
		for(i = 0; i < m; i++) number[i] = sc.nextInt()
;		
		for(i = 0; i < m; i++) {
			for(j = 0; j < button.length / number[j]; j++ ) {
				if(button[j += j] == 0)
					button[j += j] = 1;
				else
					button[j += j] = 0;
			}
		}
		
		
		
	}
}
