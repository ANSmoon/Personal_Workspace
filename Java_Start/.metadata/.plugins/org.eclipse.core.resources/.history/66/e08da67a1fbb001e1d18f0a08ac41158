package test03_throws;

// UncheckedException
// 빨간 줄 없음
// => 예외 처리 강제 x
// => throws도 강제 x
// => 하고 싶다면 try catch 진행
public class ThrowTest2 {
	public static void main(String[] args) {
		method1();
	}
	
	public static void method1() {
		method2();
	}
	
	public static void method2() {
		int i = 1 / 0;
	}
}
