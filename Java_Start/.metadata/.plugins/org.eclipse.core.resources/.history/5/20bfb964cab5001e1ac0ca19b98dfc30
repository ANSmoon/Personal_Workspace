package homework;

public class RestaurantTest {
	public static void main(String[] args) {
		String star = "*";
		Restaurant store1 = new Restaurant(100, "광양숯불구이", "서울", "고기",4);
		Restaurant store2 = new Restaurant(200, "부산회센터", "부산", "우럭",2);
		
		Review store3 = new Review(1,300,"아이유","좋은날");
		Review store4 = new Review(2, 400, "Twice","TT");
		
		System.out.println(star.repeat(40) + "맛집목록" + star.repeat(40));
		System.out.println(store1.toString());		
		System.out.println(store2.toString());
		System.out.println(star.repeat(40) + "리뷰목록" + star.repeat(40));
		System.out.println(store3.toString());		
		System.out.println(store4.toString());
	}
}
