public class String_03_문자열숫자변환 {
	public static void main(String[] args) {
		String str = "안녕123";
		int result = atoi(str);
		System.out.println(result);
	}

	static int atoi(String str) {
		int value = 0;

		for (int i = 0; i < str.length(); i++) {
			char c = str.charAt(i);
			if('0' <= c && c <='9') {
				value = (value * 10) + (c-'0');
			}else {
				System.out.println("정수가 아님");
				return -1;
			}
			
		}
		return value;
	}
}