import java.util.Scanner;

class Solution_1 {
	public static void main(String[] args) throws Exception {
		Scanner sc = new Scanner(System.in);



		for(int k = 1; k < 11; k++){
			int N = sc.nextInt();
			int arr[] = new int[N];
			int max = 0;
			int cnt = 0;
			
			for (int i = 0; i < N; i++) {
				arr[i] = sc.nextInt();
				if (arr[i] >= max)
					max = arr[i];
			}
			
			// 배열 생성
			int matrix[][] = new int[max][N];

			for (int i = 0; i < N; i++) {
				for (int j = arr[i] - 1; j >= 0; j--) {
					matrix[j][i] = 1;
				}
			}

////			 확인 코드
//			for(int i = max; i >= 0; i--) {
//				for(int j = 0;  j < N; j++) {
//					System.out.print(matrix[i][j]);
//				}
//				System.out.println();
//			}
			
			// 양쪽 검사 후 갯수 파악
			for (int i = 2; i < N - 2; i++) {
				for (int j = 0; j < max; j++) {
					if (matrix[j][i] + matrix[j][i - 1] + matrix[j][i - 2] + matrix[j][i + 1] +
							 matrix[j][i + 2] == 1) {
						cnt++;
					}
				}
			}
			
			System.out.printf("#%d %d\n", k, cnt);
		}
		sc.close();
	}
}
