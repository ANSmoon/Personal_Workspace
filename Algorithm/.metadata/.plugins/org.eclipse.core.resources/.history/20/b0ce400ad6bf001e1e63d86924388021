package defaul;
import java.util.Scanner;

public class Array3_02_deltaArray {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);

		int[][] arr = new int[5][5];
		int pr = 2;
		int pc = 2;
		int[] dr = { -1, 1, 0, 0 };
		int[] dc = { 0, 0, -1, 1 };

		while (true) {
			int d = sc.nextInt();

			pr = pr + dr[d];
			pc = pc + dc[d];
		}
	}
}
